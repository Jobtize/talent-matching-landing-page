{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_VERCEL_ENV": "preview"
  },
  "github": {
    "enabled": true,
    "silent": false,
    "autoAlias": true,
    "production": {
      "branch": "main"
    },
    "preview": {
      "branch": "preview"
    }
  }
}
